{
  "name": "micro-saml-mock",
  "description": "saml auth mock microservice",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Jonas Enge",
    "email": "jonas.enge@gmail.com",
    "url": "https://github.com/maccyber"
  },
  "contributors": [
    {
      "name": "Jonas Enge",
      "email": "geir.gasodden@pythonia.no",
      "url": "https://github.com/zrrrzzt"
    }
  ],
  "main": "index.js",
  "engines": {
    "node": ">=7.9.0"
  },
  "scripts": {
    "test": "standard && nsp check && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "setup": "npm install",
    "standard-fix": "standard --fix",
    "start": "micro",
    "now-deploy": "npm test && now --npm && now alias"
  },
  "keywords": [
    "microservice",
    "micro",
    "mock",
    "auth",
    "Telemark fylkeskommune"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/telemark/micro-saml-mock.git"
  },
  "bugs": {
    "url": "https://github.com/telemark/micro-saml-mock/issues"
  },
  "homepage": "https://github.com/telemark/micro-saml-mock",
  "now": {
    "alias": [
      "auth.demo.t-fk.win"
    ],
    "type": "npm",
    "env": {
      "NODE_ENV": "production"
    }
  },
  "devDependencies": {
    "ava": "0.19.1",
    "coveralls": "2.13.0",
    "nsp": "2.6.3",
    "nyc": "10.2.0",
    "standard": "10.0.2",
    "test-listen": "1.0.2"
  },
  "dependencies": {
    "axios": "0.16.1",
    "jsonwebtoken": "7.4.0",
    "marked": "0.3.6",
    "micro": "7.3.2",
    "winston": "2.3.1",
    "winston-papertrail": "1.0.4",
    "simple-encryptor": "1.1.0",
    "urlencoded-body-parser": "2.0.1"
  }
}
